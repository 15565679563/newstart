<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wlu.newstart.dao.UserDao">
    <insert id="addUser" parameterType="com.wlu.newstart.entity.UserEntity">
        insert into user(id,username,loginId,password,tel) values (#{id},#{userName},#{loginId},#{password},#{tel})
    </insert>
</mapper>